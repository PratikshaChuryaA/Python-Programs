class Solution:
    def isAnagram(self, a, b):
        # Sort the strings and compare them
        sorted_a = sorted(a)
        sorted_b = sorted(b)
        
        if sorted_a == sorted_b:
            return True
        else:
            return False
    




#{ 
 # Driver Code Starts
#Initial Template for Python 3

if __name__=='__main__':
    t = int(input())
    for i in range(t):
        a,b=map(str,input().strip().split())
        if(Solution().isAnagram(a,b)):
            print("YES")
        else:
            print("NO") 
